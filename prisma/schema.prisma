generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

model User {
  userKey     Int      @id @default(autoincrement())
  email       String   @unique
  firstName   String
  password    String
  salt        String
  createdDate DateTime @default(now())
}
